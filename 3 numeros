#include <stdio.h>

int main()
{
    int x,
        y,
        z,
        maior;
            printf("\n Programa de Guilherme Cardoso");// identificação do Autor   
        	printf("\n para apresentacao do trabalho 02 ex. 05");   
	        printf("\n uso do if else para entrada de dados ");

    printf("Digite o numero 1: ");
    scanf("%d", &x);

    printf("Digite o numero 2: ");
    scanf("%d", &y);
    
    printf("Digite o numero 3: ");
    scanf("%d", &z);

    if(x > y && x > z){
        maior = x;
        
    }
        else if (y > x && y > z){
            maior = y;
        
        }
            else{
                maior = z;
            }

    printf("O maior numero eh: %d\n", maior);
}
